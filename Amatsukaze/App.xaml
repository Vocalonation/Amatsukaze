<Application x:Class="Amatsukaze.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Amatsukaze"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <Color x:Key="AmatsukazeBrown">#402D2F</Color>
        <Color x:Key="AmatsukazeRed">#B13136</Color>
        <Color x:Key="AmatsukazeWhite">#FFFFFF</Color>         
        <SolidColorBrush x:Key="BaseColor" 
                         Color="{StaticResource AmatsukazeBrown}" />
    </Application.Resources>
</Application>
